    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.11",
    "react-icons": "^4.2.0"
  }
